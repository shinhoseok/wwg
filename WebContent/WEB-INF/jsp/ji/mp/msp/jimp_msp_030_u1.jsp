<%@page pageEncoding="utf-8"%><%//jimp_msp_030_u1.jsp%><script type="text/javascript" src="<%=con_root%>/js/jquery.form.min.js"  charset="utf-8"></script><form name="dataForm" id="dataForm" method="post" enctype="multipart/form-data"><input type="hidden" name="scode" 						id="scode" 				value="<%=scode%>" title="사이트코드" /><input type="hidden" name="pcode" 						id="pcode" 				value="<%=pcode%>" title="페이지코드"  /><input type="hidden" name="pstate" 						id="pstate" 			value="<%=pstate%>" title="페이지상태"  /><input type="hidden" name="curr_page" 					id="curr_page" 			value="<%=curr_page%>" title="현재페이지번호"  /><input type="hidden" name="show_rows" 					id="show_rows" 			value="<%=show_rows%>" title="현재페이지당데이터수"  /><input type="hidden" name="sGbOption" 					id="sGbOption" 			value="<c:out value='${R_MAP.param.sGbOption}'/>" /><input type="hidden" name="sKeyWord" 					id="sKeyWord" 			value="<c:out value='${R_MAP.param.sKeyWord}'/>" /><input type="hidden" name="sDate" 						id="sDate" 				value="<c:out value='${R_MAP.param.sDate}'/>" /><input type="hidden" name="eDate" 						id="eDate" 				value="<c:out value='${R_MAP.param.eDate}'/>" /><input type="hidden" name="idec_seqno" 					id="idec_seqno"			value="<c:out value='${R_MAP.param.idec_seqno}'/>" /><!--supply_box--><div class="supply_box">		<!--//board_A0_write-->	<div class="board_A0_V">		<h3 class="fL"><span>&#8226;</span>심의위원회 심의차수관리(수정)</h3>		<table summary="심의위원회 심의차수관리(수정)">			<caption>심의위원회 심의차수관리(수정)-심의구분,심의일자,심의차수,심의사유,심의관련파일</caption>			<colgroup>				<col width="200px" />				<col width="*" />				<col width="200px" />				<col width="*" />				<col width="200px" />				<col width="*" />			</colgroup>			<tbody>				<tr>					<th scope="row">심의구분</th>					<td >						<select name="dec_gbn" id="dec_gbn" style="background:#cccccc;" title="심의구분 선택">							<option value="" <c:if test="${R_MAP.decDtl.dec_gbn == ''}">selected</c:if> >--- 선택 ---</option>							<option value="1" <c:if test="${R_MAP.decDtl.dec_gbn == '1'}">selected</c:if> >제공심의</option>							<option value="5" <c:if test="${R_MAP.decDtl.dec_gbn == '5'}">selected</c:if> >반출심의</option>						</select>										</td>					<th scope="row">심의일자</th>					<td>						<input type="text" class="input_type01 w_150 f_l" id="dec_dt" name="dec_dt" maxlength="30" value="<c:out value="${fn:substring(R_MAP.decDtl.dec_dt, 0, 4)}"/>-<c:out value="${fn:substring(R_MAP.decDtl.dec_dt, 4, 6)}"/>-<c:out value="${fn:substring(R_MAP.decDtl.dec_dt, 6, 8)}"/>" readonly style="background:#cccccc;" />					</td>					<th scope="row">심의차수</th>					<td>						<input type="text" class="input_type01 w_150" id="dec_step_num" name="dec_step_num" maxlength="2" onkeyup="inputNum(this);" value="<c:out value="${R_MAP.decDtl.dec_step_num}"/>" readonly style="IME-MODE:disabled;background:#cccccc;"  />					</td>				</tr>								<tr>					<th scope="row">심의사유</th>					<td colspan="5">						<textarea class="textarea_type01 w_100p" name="dec_rsn" id="dec_rsn" rows="5"><c:out value="${R_MAP.decDtl.dec_rsn}"/></textarea>									</td>								</tr>				<tr>					<th scope="row">심의관련파일</th>					<td colspan="5">						<div id="cmfile1"></div>									</td>								</tr>			</tbody>		</table>	</div>	<!--//board_A0_write-->		<!--button-->	<div class="button a_r mat_30">		<a href="#none" class="btn_1 size_n" id="btn_sava">수정</a>		<a href="#none" class="btn_1 size_n" id="btn_remove">삭제</a>		<a href="#none" class="btn_2 size_n" id="btn_list">목록</a>	</div>	<!--//button--></div><!--//supply_box--></form><script language="javascript" src="<%=con_root%>/js/com-file.js"></script><script type="text/javascript">	//TODO : $(function()	$(function(){		/*$("#dec_dt").datepicker({	        changeMonth: true, 	        changeYear: true,	        //minDate: 1,	        monthNames: ['1월','2월','3월','4월','5월','6월','7월','8월','9월','10월','11월','12월'],	        showOn:'both',	        dateFormat: "yy-mm-dd",	        buttonImage:'/images/contents/cal.png',	        onClose:function(selectedDate){	        		        }		});	*/		var file = $.fn.cmfile.init({			 id				: 'cmfile1' 			 ,onClickNamefn	: "fileNameClick1"			 ,img_url 		: "<%=img_url%>"			 ,uploadcnt		: <%=uploadcnt%>			 ,extchk		: ['GIF','JPG','BMP','PNG','HWP','XLS','XLSX','ZIP','PDF','TIF','DOC','DOCX','PPT','PPTX','TXT']		});				//첨부파일 세팅		var file_group = <c:if test="${R_MAP.fileListHtmlArray == ''}">""</c:if><c:out value="${R_MAP.fileListHtmlArray}" escapeXml="false"/>;		$.fn.cmfile.setfileList("cmfile1", file_group);					// 목록		$("#btn_list").click(function(e){			$("#dataForm").attr("enctype","");			$("#dataForm").find("#pstate").val("L");			$("#dataForm").submit();		});				// 저장		$("#btn_sava").click(function(e){						if(isEmpty($("#dec_gbn").val())){				alert("심의구분을 선택해주세요.");				$("#dec_gbn").focus();				return false;							}						if(isEmpty($("#dec_dt").val())){				alert("심의일자를 선택해주세요.");				$("#dec_dt").focus();				return false;							}						if(isEmpty($("#dec_step_num").val())){				alert("심의차수를 입력해주세요.");				$("#dec_step_num").focus();				return false;							}							if(isEmpty($("#dec_rsn").val())){				alert("심의사유를 입력해주세요.");				$("#dec_rsn").focus();				return false;							}												if(confirm("수정하시겠습니까?")){								CmopenpageDisable();				$("#dataForm").find("#pstate").val("U");							////////////////////////////데이터 정리				var params = jQuery("#dataForm").serialize();								jQuery("#dataForm").ajaxSubmit({			    		type: "post",			            url: "<c:url value='/cmsajax.do'/>",			            data: params,			            async: false,			            dataType:"json",			            success: function(data){			            	if(data.result==true){			                	alert('심의차수가 수정 되었습니다.');			                	$("#btn_list").trigger("click");			               	}else{								if(data.TRS_MSG!=""){									alert(''+data.TRS_MSG);								}else{									alert('심의차수 수정중 오류가 발생했습니다.1');								}			            	   CmcloseLoading();			                }			             },					     error:function(request, status, error) {					    	 CmcloseLoading();					    	 alert('심의차수 수정중 오류가 발생했습니다2');			             }			    });				}		});				$("#btn_remove").click(function(e){			if(confirm("삭제하시겠습니까?")){								CmopenpageDisable();				$("#dataForm").find("#pstate").val("D");							////////////////////////////데이터 정리				var params = jQuery("#dataForm").serialize();								jQuery("#dataForm").ajaxSubmit({			    		type: "post",			            url: "<c:url value='/cmsajax.do'/>",			            data: params,			            async: false,			            dataType:"json",			            success: function(data){			            	if(data.result==true){			                	alert('심의차수가 삭제 되었습니다.');			                	$("#btn_list").trigger("click");			               	}else{								if(data.TRS_MSG!=""){									alert(''+data.TRS_MSG);								}else{									alert('심의차수 삭제중 오류가 발생했습니다.1');								}			            	   CmcloseLoading();			                }			             },					     error:function(request, status, error) {					    	 CmcloseLoading();					    	 alert('심의차수 삭제중 오류가 발생했습니다2');			             }			    });				}							});				$("select[name=dec_gbn] option:not(:selected)").remove();			});		</script>