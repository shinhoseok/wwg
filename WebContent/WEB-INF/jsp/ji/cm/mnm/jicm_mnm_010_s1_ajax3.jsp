$(this).val();			} else {				params.boardEtcItemNm = params.boardEtcItemNm + comFlag + $(this).val();			}		});	} else if(params.linkTy === "000009") { //프로그램		params.classPath = $("#updateTable #classPath").val();		params.classPathJsp = $("#updateTable #classPathJsp").val();		params.classMethod = $("#updateTable #classMethod").val();		params.jspIncludeYn = $("#updateTable #jspIncludeYn option:selected").val();	}	//메뉴관리 트리에서 현재 선택한 메뉴(상위메뉴)	params.menuUppoCd = $("#menuMngTree").children().find(".jstree-clicked").parent().attr("menuCd");		$.ajax({		url: "<c:out value='${basePath}'/>/admin/menu/updateMenuProc.do",		type: 'POST',		dataType : "json",		data : params,		error: function(){			 alert("현재 등록 서비스가 원활하지 않습니다.\n관리자에게 문의주시기 바랍니다.");			 return;		},		success: function(r) {			menuUppoCd = params.menuUppoCd;			fn_selectMngMenuTree();			alert("정상적으로 수정되었습니다.");			var offset = $("#menuMngTreeTarget").offset();			$("html, body").animate({scrollTop : offset.top}, 400);		}	});};</script>