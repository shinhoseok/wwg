<%@ page contentType="text/html; charset=utf-8" pageEncoding="utf-8" %><%@include file="/WEB-INF/jsp/cm/include.jsp"%><div class="new_mnb" id="menuMngTree" style="overflow-x: scroll; width: 260px;">	<ul>		<c:forEach items="${menuMngList}" var="oneMenuList" varStatus="i">			<li menuCd="<c:out value="${oneMenuList.menuCd }" />"><c:out value="${oneMenuList.menuNm }" />				<ul>					<c:forEach items="${oneMenuList.menuList}" var="twoMenuList" varStatus="j">						<li menuCd="<c:out value="${twoMenuList.menuCd }" />"><c:out value="${twoMenuList.menuNm }" />							<ul>								<c:forEach items="${twoMenuList.menuList}" var="threeMenuList" varStatus="z">									<li menuCd="<c:out value="${threeMenuList.menuCd }" />"><c:out value="${threeMenuList.menuNm }" /></li>								</c:forEach>							</ul>						</li>					</c:forEach>				</ul>			</li>		</c:forEach>	</ul></div><script>$(document).ready(function() {	$("#menuMngTree").jstree({		"core" : {"dblclick_toggle" :  false}	// 더블클릭 이벤트 제거	}).on('ready.jstree', function (e, data) {		//$("li[menucd='000001']")		$("#menuMngTree").jstree("open_all");		if(menuUppoCd !== "") {			data.instance.open_node($("li[menuCd='"+menuUppoCd+"']"));	// 메뉴 수정 시 열려 있던 메뉴 노드 열기	 		data.instance.select_node($("li[menuCd='"+menuUppoCd+"']")); // 메뉴 수정 시 열려 있던 메뉴 노드 선택		} else {			data.instance.open_node($("#j2_1_anchor")); //첫번째 노드 오픈			data.instance.select_node($("#j2_1_anchor")); //첫번째 노드 선택		}	}).on("select_node.jstree", function (e, data) {		data.instance.open_node([ data.node.id ]); //선택한 노드 열기		fn_selectMenuDetail(data.node.li_attr.menucd);	}).init("loaded.jstree", function (e, data) {		$("#menuMngTree").show();	});});fn_selectMenuDetail = function(menuCd) {	$.ajax({		url: "<c:out value='${basePath}'/>/admin/selectMenuDetail.do",		type: 'POST',		dataType : "html",		data : {"menuCd" : menuCd},		error: function(){			 alert("현재 조회 서비스가 원활하지 않습니다.\n관리자에게 문의주시기 바랍니다.");			 return;		},		success: function(r) {			$("#menuDetailTarget").children().remove();			$("#menuDetailTarget").html(r);// 			$("#menuGubun").text();		}	});};</script>