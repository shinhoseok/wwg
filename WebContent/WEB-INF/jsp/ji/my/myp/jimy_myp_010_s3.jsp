<%@page pageEncoding="utf-8"%>	<form action="<%=RequestURL%>" method="post" id="dataForm" name="dataForm"><input type="hidden" id="scode"		name="scode" 	value="<%=scode %>" /><input type="hidden" id="pcode"		name="pcode" 	value="<%=pcode %>" /><input type="hidden" id="pstate"	name="pstate" 	value="<%=pstate %>" /><input type="hidden" id="page"		name="page" value=""  title="페이지"  /><input type="hidden" id="idea_propse_sn"	name="idea_propse_sn" value=""  title="아이디어 제안 일련번호" /><input type="hidden" id="password"			name="password" 	  value=""  title="아이디어 제안 입력 패스워드" /><h5 class="main_title">    <strong><%=M_PATH_STR.substring(M_PATH_STR.lastIndexOf("::")+2)%></strong>    <p class="copy">전력데이터 개방 포털시스템 <%=M_PATH_STR.substring(M_PATH_STR.lastIndexOf("::")+2)%>입니다.</p></h5><!--tab--><div class="tab_A0 a_c split_4 mab_30">	<ul>		<li id="tab1"><a href="#;" onClick="map_tab('1'); return false;">회원정보수정</a></li>		<li id="tab2"><a href="#;" onClick="map_tab('2'); return false;">맞춤 데이터 제공 요청 목록</a></li>		<li id="tab3" class="on"><a href="#;" onClick="map_tab('3'); return false;">아이디어 제안 목록</a></li>		<!-- <li id="tab4" ><a href="#;" onClick="map_tab('4'); return false;">분석인프라 신청 목록</a></li> -->		<li id="tab4" ><a href="https://bigdata-bi.kepco.co.kr:9503/analytics/" target="_blank" >BI연결</a></li>	</ul></div><!--//tab--><!--view3--><div class="map_box">	<!--board_A0_list-->	<div class="board_A0_L">		<p class="count">			[총 <b><c:out value="${R_MAP.paginationInfo.totalRecordCount}"/></b>건, <!-- 페이지 CONUNT -->				<b><c:out value="${R_MAP.paginationInfo.currentPageNo}"/></b>   	<!-- 선택된 페이지 -->				/ <c:out value="${R_MAP.paginationInfo.lastPageNo}"/>				<!-- 마지막 페이지 -->			]		</p>		<table summary="아이디어 제안">			<caption>아이디어 제안-번호,제목,첨부,작성자,등록일,승인여부</caption>			<colgroup>				<col width="" />				<col width="" />				<col width="" />				<col width="" />				<col width="10%" />				<col width="10%" />			</colgroup>			<thead>				<tr>					<th scope="col" class="resp">번호</th>					<th scope="col">제목</th>					<th scope="col" class="resp">첨부</th>					<th scope="col" class="resp">작성자</th>					<th scope="col" class="resp">등록일</th>					<th scope="col" >승인여부</th>				</tr>			</thead>			<tbody>				<c:if test="${empty R_MAP.ideaPropseList}">					<tr class="secret">						<td colspan="6">조회된 아이디어 제안 목록이 없습니다.</td>					</tr>				</c:if>				<c:if test="${!empty R_MAP.ideaPropseList}">					<c:forEach var="list" items="${R_MAP.ideaPropseList}" varStatus="i">						<tr <c:if test="${list.password eq 'true'}">class="secret"</c:if>>							<td class="resp"><c:out value=" ${list.rowNum}"/></td>							<td class="subject">								<a href="#" onclick="passCheck('<c:out value='${list.ideaPropseSn}'/>');return false;">									<edPotal:cutString contents="${list.ideaTitle}" length="45"  />								</a>																<!-- 								<c:choose>									<c:when test="${list.IdeaPropseStatusNm eq '제안' }">										<span class="icon_new">새글</span>									</c:when>									<c:otherwise>																			</c:otherwise>								</c:choose>								 -->								 								<span class="icon_new">새글</span>							</td>							<td class="resp add">								<c:choose>									<c:when test="${!empty list.fileSeq}">										<a href="#" onclick="goDownfile('<c:out value='${list.fileSeq}'/>');return false;" title="">											<span></span>										</a>									</c:when>									<c:otherwise>																			</c:otherwise>								</c:choose>							</td>														<td class="resp"><c:out value="${list.userNm}"/></td>							<td class="resp"><c:out value="${list.regDt}"/></td>							<td>								<c:if test="${!empty list.ideaRejectSttus}"><span class=stk_7><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '제안'}"><span class=stk_1><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '접수'}"><span class="stk_2"><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '심의예정'}"><span class="stk_3"><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '채택'}"><span class="stk_4"><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '사업검토'}"><span class="stk_5"><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>								<c:if test="${list.ideaPropseStatusNm eq '매칭완료'}"><span class="stk_6"><c:out value="${list.ideaPropseStatusNm}"/></span></c:if>							</td>						</tr>					</c:forEach>				</c:if>			</tbody>		</table>	</div>	<!--//board_A0_list-->		<!--paging-->	<div class="paging">		<ui:pagination paginationInfo="${R_MAP.paginationInfo}" type="common" jsFunction="goPage" />	</div>	<!--paging-->	</div><!--//view3-->			</form>					<script language="javascript" src="<%=con_root%>/js/com-file.js"></script><script type="text/javascript">//<![CDATA[           	function map_tab(va) {		if (va == '1') {			$("input[name=pstate]").val("L");		}		if (va == '2') {			$("input[name=pstate]").val("L2");		}		if (va == '3') {			$("input[name=pstate]").val("L3");		}		if (va == '4') {			$("input[name=pstate]").val("L4");		}		$("#dataForm").submit();	}		// 패스워드 확인 팝업창 띄우기	function passCheck(key) {				<%-- var vUrl = "<%=RequestURL%>?scode=000008&pcode=000413&callbackFnc=popupCallback&pstate=PASSINFO&idea_propse_sn="+ key; --%>		/* popWinScroll(vUrl, "searchDataPop", 500, 550); */		//비밀번호확인 레이어호출		fnOpenPwLayer(key, "OPEN");	    $("#idea_propse_sn").val(key);	}		//팝업호출후콜백함수	function fnCallBack(obj){				goIdeaPropse();	}		// 데이터 목록 선택	//function goIdeaPropse(ideaPropseId){	function goIdeaPropse(){				var frm = document.dataForm;		frm.method       = 'post';		frm.pstate.value = 'R';		frm.action       = '<%=RequestURL%>';		frm.submit();	}		// page	var goPage = function(pageNo){				var frm = document.dataForm;				$("#page").val(pageNo);				frm.method       = 'post';		frm.pstate.value = 'L3';		frm.action       = '<%=RequestURL%>';		frm.submit();	};		// 다운로드	var goDownfile = function(file_seqno){		$.fn.cmfile.fileDownLoad("", file_seqno);	};//]]></script>