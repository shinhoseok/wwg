<%@page pageEncoding="utf-8"%>	<form action="<%=RequestURL%>" method="post" id="dataForm" name="dataForm"><input type="hidden" id="scode"					name="scode" 				value="<%=scode %>" /><input type="hidden" id="pcode"				name="pcode" 				value="<%=pcode %>" /><input type="hidden" id="pstate"				name="pstate" 				value="<%=pstate %>" /><input type="hidden" id="page" 					name="page" 				value="${R_MAP.param.page}"/><input type="hidden" id="ifrm_rcept_no"	name="ifrm_rcept_no" /><h5 class="main_title">    <strong><%=M_PATH_STR.substring(M_PATH_STR.lastIndexOf("::")+2)%></strong>    <p class="copy">전력데이터 개방 포털시스템 <%=M_PATH_STR.substring(M_PATH_STR.lastIndexOf("::")+2)%>입니다.</p></h5>			<!--tab--><div class="tab_A0 a_c split_4 mab_30">	<ul>		<li id="tab1"><a href="#;" onClick="map_tab('1'); return false;">회원정보수정</a></li>		<li id="tab2" class="on"><a href="#;" onClick="map_tab('2'); return false;">맞춤 데이터 제공 요청 목록</a></li>		<li id="tab3" ><a href="#;" onClick="map_tab('3'); return false;">아이디어 제안 목록</a></li>		<!-- <li id="tab4" ><a href="#;" onClick="map_tab('4'); return false;">분석인프라 신청 목록</a></li> -->		<li id="tab4" ><a href="https://bigdata-bi.kepco.co.kr:9503/analytics/" target="_blank" >BI연결</a></li>	</ul></div><!--//tab--><div class="map_box">		<!--board_A0_list-->	<div class="board_A0_L">		<p class="count">[총 <b><c:out value="${R_MAP.paginationInfo.totalRecordCount}"/></b>건, <b><c:out value="${R_MAP.param.page}"/></b>/<c:out value="${R_MAP.paginationInfo.lastPageNo}"/>]</p>		<table summary="">			<caption>번호,제목,상태,작성자,조회수,작성일</caption>			<colgroup>				<col width="100" />				<col width="*" />				<col width="200" />				<col width="" />				<col width="" />				<col width="" />				<!-- <col width="" /> -->				<!-- <col width="50" /> -->				<col width="150" />			</colgroup>			<thead>				<tr>					<th scope="col" class="resp">번호</th>					<th scope="col">제목</th>					<th scope="col">상태</th>					<th scope="col" class="resp">작성자</th>					<th scope="col" class="resp">조회수</th>					<th scope="col" class="resp">작성일</th>				</tr>			</thead>			<tbody>				<c:if test="${empty R_MAP.list}">					<tr class="secret">						<td colspan="9">조회된 맞춤데이터 제공 요청목록이 없습니다.</td>					</tr>				</c:if>				<c:if test="${!empty R_MAP.list}">					<c:forEach var="list" items="${R_MAP.list}" varStatus="status">						<tr <c:if test="${list.othbcYn == 'N'}">class="secret"</c:if>>							<td class="resp"><c:out value="${R_MAP.paginationInfo.totalRecordCount - list.rnum + 1}"/></td>							<td class="subject">								<a href="#" onclick="fnIfrmRequstInfo('<c:out value='${list.ifrmRceptNo}'/>', '<c:out value='${list.othbcYn}'/>', '<c:out value='${list.regId}'/>');return false;">									<edPotal:cutString contents="${list.ifrmRequstSj}" />								</a>								<c:if test="${list.newFlag == 'Y'}">									<!-- 1주일 기준 -->									<span class="icon_new">새글</span>								</c:if>							</td>							<td>								<!--									  요청		:	000240									  접수		:	000241									  보완요청	:	000242									  처리중 	:	000243									  완료		:	000244								-->								<span <c:choose>												<c:when test="${list.requstSttusCd == '000240'}">													class="stk_5"												</c:when>												<c:when test="${list.requstSttusCd == '000241'}">													class="stk_1"												</c:when>												<c:when test="${list.requstSttusCd == '000242'}">													class="stk_4"												</c:when>												<c:when test="${list.requstSttusCd == '000243'}">													class="stk_3"												</c:when>												<c:when test="${list.requstSttusCd == '000244'}">													class="stk_2"												</c:when>												</c:choose> 								>								<c:out value="${list.requstSttusCdNm}"/>								</span>							</td>							<td class="resp"><c:out value="${list.userNm}"/></td>							<td class="resp"><c:out value="${list.rdcnt}"/></td>							<td class="resp"><c:out value="${list.regDt}"/></td>						</tr>					</c:forEach>				</c:if>			</tbody>		</table>	</div>	<!--//board_A0_list-->		<!--paging-->	<div class="paging">		<ui:pagination paginationInfo="${R_MAP.paginationInfo}" type="common" jsFunction="goPage" />	</div>	<!--paging-->	</div><!--//view2-->			</form><script language="javascript" src="<%=con_root%>/js/com-file.js"></script><script type="text/javascript">//<![CDATA[  		// 상세보기	var fnIfrmRequstInfo = function(ifrmRceptNo, ifrnScrtyOthbcAt){				$("#dataForm").find("#ifrm_rcept_no").val(ifrmRceptNo);				if(ifrnScrtyOthbcAt == 'N'){		   fnOpenPwLayer(ifrmRceptNo, "OUT");		}else{			$("#dataForm").find("#pcode").val("000071");			$("#dataForm").find("#pstate").val("DF");			$("#dataForm").submit();		}	};		// page	var goPage = function(pageNo){		$("#dataForm").find("#page").val(pageNo);		$("#dataForm").find("#pstate").val("L2");		$("#dataForm").submit();	};		function map_tab(va) {		if (va == '1') {			$("input[name=pstate]").val("L");		}		if (va == '2') {			$("input[name=pstate]").val("L2");		}		if (va == '3') {			$("input[name=pstate]").val("L3");		}		if (va == '4') {			$("input[name=pstate]").val("L4");		}		$("#dataForm").submit();	}		var fnCallBack = function(){		$("#dataForm").find("#pstate").val("DF");		$("#dataForm").submit();	};		// 다운로드	var goDownfile = function(file_seqno){		$.fn.cmfile.fileDownLoad("", file_seqno);	};	//]]></script>