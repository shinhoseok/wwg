").InnerHTML = document.getElementsByName("CONTENTS_STR")[0].value;	<%	if(!C_CSS.equals("")){	%>	$('#contents').parent('div').addClass('<%=C_CSS%>');	<%	}	%>	<%	if(!C_JSCRIPT.equals("")){	%>	<%=C_JSCRIPT.replaceAll("&amp;", "&").replaceAll("&apos;", "'").replaceAll("&quot;", "\"").replaceAll("&lt;", "").replaceAll("&gt;", "")%>	<%	}	%>	});//]]></script><!--내용끝-->